{
  "name": "react-formation",
  "version": "1.0.0",
  "main": "./lib/index.js",
  "description": "Form automation and validation builder",
  "repository": "git@github.com:StephenGrider/ReduxSimpleStarter.git",
  "scripts": {
    "start": "node ./node_modules/webpack-dev-server/bin/webpack-dev-server.js",
    "build:es2015": "cross-env NODE_ENV=production babel ./src --ignore *.spec.js --out-dir ./build",
    "build:es2015modules": "cross-env NODE_ENV=production BABEL_ENV=modules babel ./src/index.js --out-file ./build/index.es.js",
    "build:es": "cross-env NODE_ENV=production BABEL_ENV=es babel ./src --ignore *.spec.js --out-dir ./build/es",
    "build:copy-files": "babel-node ./scripts/copy-files.js",
    "build:umd:dev": "webpack --config scripts/umd.webpack.config.js",
    "build:umd:prod": "cross-env NODE_ENV=production webpack --config scripts/umd.webpack.config.js",
    "build": "yarn build:es2015 && yarn build:copy-files && npm --prefix ./build install",
    "build-mui": "yarn build:es2015 && yarn build:es2015modules && yarn build:es && yarn build:copy-files && yarn build:umd:dev && yarn build:umd:prod",
    "build2": "babel src --out-dir lib --ignore __tests__",
    "watch": "cross-env NODE_ENV=production babel ./src --ignore *.spec.js --out-dir ./build --watch",
    "test": "mocha --compilers js:babel-core/register --require ./test/test_helper.js --recursive ./test",
    "test:watch": "npm run test -- --watch"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "babel-preset-stage-1": "^6.1.18",
    "email-validator": "^1.0.7",
    "es6-enum": "^1.1.0",
    "es6-pick": "^1.0.0",
    "fast-deep-equal": "^1.0.0",
    "intl": "^1.2.5",
    "intl-locales-supported": "^1.0.0",
    "is-promise": "^2.1.0",
    "lodash": "^4.17.4",
    "lodash.debounce": "^4.0.8",
    "map-props": "^1.0.0",
    "material-ui": "^0.20.0",
    "path": "^0.12.7",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-pure-render": "^1.0.2",
    "react-redux": "^5.0.5",
    "react-router-dom": "^4.1.1",
    "react-tap-event-plugin": "^3.0.0",
    "react-toggle-display": "^2.2.0",
    "recompose": "^0.24.0",
    "reduce-reducers": "^0.1.2",
    "redux": "^3.7.0",
    "redux-actions": "^2.2.1",
    "redux-logger": "2.4.0",
    "redux-promise": "^0.5.3",
    "redux-thunk": "1.0.3",
    "valid-url": "^1.0.9"
  },
  "devDependencies": {
    "axios": "^0.16.1",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.2",
    "babel-loader": "^7.0.0",
    "babel-plugin-istanbul": "^4.1.1",
    "babel-plugin-lodash": "^3.2.11",
    "babel-plugin-syntax-async-functions": "^6.13.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.24.1",
    "babel-plugin-transform-regenerator": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2015-no-commonjs": "^0.0.2",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.24.1",
    "chai": "^3.5.0",
    "chai-jquery": "^2.0.0",
    "cross-env": "^5.0.1",
    "eslint-config-rallycoding": "^3.1.0",
    "flow-copy-source": "^1.2.2",
    "fs-extra": "^5.0.0",
    "jquery": "^2.2.1",
    "jsdom": "^8.1.0",
    "json-loader": "^0.5.7",
    "mocha": "^2.4.5",
    "react-addons-test-utils": "^0.14.7",
    "webpack": "^1.12.12",
    "webpack-dev-server": "^1.14.0"
  }
}
